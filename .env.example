# Gemini API Key for AI model (used for background tasks: memory evolution only)
GEMINI_API_KEY=your_gemini_api_key_here

# LLM API Configuration (used for user chat responses and intent classification)
LLM_API_URL=https://your-llm-api-url/v1/chat/completions
LLM_MODEL=/path/to/your/model
NODE_TLS_REJECT_UNAUTHORIZED=0

# Next.js Configuration
PORT=3001

# Upstash Redis Configuration
# Get these values from your Upstash console: https://console.upstash.com/
# 1. Create a new Redis database
# 2. Copy the REST URL and REST Token from the database details
KV_REST_API_URL=your_kv_rest_api_url
KV_REST_API_TOKEN=your_kv_rest_api_token
